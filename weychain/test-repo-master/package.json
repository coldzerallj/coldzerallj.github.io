{
	"version": "1.0.0",
	"license": "ISC",
	"description": "Project",
	"author": "weychain fed",
	"private": true,
	"scripts": {
		"start": "cross-env ENV=local node build/npm-script-config.js",
		"build:dev": "cross-env ENV=dev node build/build.js",
		"build:sit": "cross-env ENV=sit node build/build.js",
		"build:uat": "cross-env ENV=uat node build/build.js",
		"build:stage": "cross-env ENV=stage node build/build.js",
		"build:prod": "cross-env ENV=prod node build/build.js",
		"changelog": "conventional-changelog -p vue -i CHANGELOG.md -s",
		"svg": "svg-sprite --symbol --dest=static --symbol-dest=icon --symbol-sprite=symbol.svg src/svg/*.svg",
		"test": "jest"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"vue"
		],
		"transform": {
			".*\\.(vue)$": "vue-jest",
			"^.+\\.js$": "<rootDir>/node_modules/babel-jest"
		},
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1"
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"{src,build,config}/**/*.{js,json,vue}": [
			"eslint --fix",
			"git add"
		],
		"src/style/**/*.scss": [
			"prettier --write",
			"git add"
		]
	},
	"dependencies": {
		"@babel/runtime": "^7.9.2",
		"@babel/runtime-corejs3": "^7.9.2",
		"axios": "^0.19.2",
		"core-js": "^3.6.4",
		"element-ui": "^2.13.0",
		"es6-promise": "^4.1.1",
		"husky": "^4.2.3",
		"js-cookie": "^2.2.1",
		"nprogress": "^0.2.0",
		"vue": "^2.3.3",
		"vue-i18n": "^8.3.1",
		"vue-router": "^2.6.0",
		"vuex": "^2.3.1"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0",
		"@babel/plugin-transform-runtime": "^7.9.0",
		"@babel/preset-env": "^7.9.5",
		"@babel/register": "^7.9.0",
		"@commitlint/cli": "^8.3.5",
		"@commitlint/config-conventional": "^8.3.4",
		"autoprefixer": "^7.1.2",
		"babel-eslint": "^8.2.3",
		"babel-jest": "^25.2.6",
		"babel-loader": "^8.0.0",
		"babel-preset-mobx": "^2.0.0",
		"chalk": "^2.0.1",
