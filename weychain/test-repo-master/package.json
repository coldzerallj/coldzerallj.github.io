		"copy-webpack-plugin": "^5.1.1",
		"cross-env": "^5.0.5",
		"css-loader": "^3.5.2",
		"cssnano": "^4.1.10",
		"eslint": "^4.19.1",
		"eslint-config-prettier": "^2.9.0",
		"eslint-plugin-prettier": "^2.6.0",
		"eslint-plugin-vue": "^4.5.0",
		"eventsource-polyfill": "^0.9.6",
